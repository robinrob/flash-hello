from enum import Enum
 
class DaysOfWeek(Enum):
    Sunday      = 0
    Monday      = {value:1, name:'Monday'}
    Tuesday     = 2
    Wednesday   = 3
    Thursday    = 4
    Friday      = 5
    Saturday    = 6
    
