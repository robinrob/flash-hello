import unittest
from my_date import Date

class TestMyDate(unittest.TestCase):
    def Test_should_not_be_leap_year(self):
        robin = Date(1987, 6, 24)
        
        self.assertFalse(robin.is_leap_year())
        
    def Test_should_be_leap_year(self):
        robin = Date(2000, 6, 24)
        
        self.assertFalse(robin.is_leap_year())