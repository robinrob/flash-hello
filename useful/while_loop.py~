drivers = ["Jenson Button", "Fernando Alonso", "Nico Rosberg", "Sebastian Vettel", "Pastor Maldonado", "Mark Webber", "Lewis Hamilton"]
i = 0
while i < len(drivers):
	print(drivers[i])
	i = i + 1

print "\n"

